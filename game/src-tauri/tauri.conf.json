{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "war2-game-game",
  "version": "0.1.0",
  "identifier": "war2.game",
  "build": {
    "frontendDist": "../build"
  },
  "app": {
