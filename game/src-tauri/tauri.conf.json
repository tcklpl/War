{
    "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "war2-game-client",
    "version": "../package.json",
    "identifier": "com.github.tcklpl.war2",
    "build": {
        "frontendDist": "../build"
    },
    "app": {
        "windows": [
            {
                "title": "War2 Game Client",
                "width": 1280,
                "height": 720,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
    }
}
